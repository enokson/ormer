{
    "ormerVersion": "1",
    "database": {
        "type": "postgres",
        "exportTs": true,
        "models": {
            "User": {
                "tableName": "users",
                "members": {
                    "uuid": "Uuid @primary @default(Uuid)",
                    "firstName": "String",
                    "lastName": "String",
                    "email": "String @unique",
                    "phoneNumber": "String",
                    "favoritePost": "Post?",
                    "posts": "Post[]",
                    "groups": "Group[]"
                },
                "queryOptions": {
                    "uuid": {
                        "filterTypes": {
                            "onlyEquals": {
                                "variants": [
                                    "equals"
                                ]
                            }
                        },
                        "sortOrder": [
                            "equals",
                            "gte",
                            "notEqual"
                        ],
                        "ignoreVariants": [
                            "lte",
                            "lt"
                        ]
                    },
                    "firstName": {
                        "ignoreVariants": [
                            "search",
                            "startsWith",
                            "endsWith"
                        ]
                    }
                }
            },
            "Post": {
                "tableName": "posts",
                "members": {
                    "uuid": "Uuid @primary @default(Uuid)",
                    "userUuid": "Uuid?",
                    "user": "   User @relation( fields: [ userUuid ], references: [ uuid ] )"
                }
            },
            "Group": {
                "members": {
                    "uuid": "Uuid @primary @default(Uuid)",
                    "users": "User[]"
                }
            }
        }
    }
}